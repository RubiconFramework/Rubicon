; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Rubicon"
run/main_scene="res://source/Rubicon.Menus/MainMenu/MainMenu.tscn"
config/features=PackedStringArray("4.3", "C#", "Forward Plus")
run/max_fps=144
config/icon="res://icon.svg"

[autoload]

ConductorSingleton="*res://source/Promise.Framework/Conductor.cs"
CoroutineManager="*res://addons/HCoroutines/CoroutineManager.tscn"
SaveData="*res://source/Rubicon.Data/SaveData.cs"

[display]

window/size/viewport_width=1366
window/size/viewport_height=768
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="Rubicon"

[editor_plugins]

enabled=PackedStringArray("res://addons/godot_sparrow/plugin.cfg")

[file_customization]

folder_colors={
"res://source/Promise.Framework/": "purple",
"res://source/Rubicon.API/": "red",
"res://source/Rubicon.Data/": "yellow",
"res://source/Rubicon.Extras/": "yellow",
"res://source/Rubicon.Game/": "yellow",
"res://source/Rubicon.Menus/": "blue",
"res://source/Rubicon.Space2D/": "blue",
"res://source/Rubicon.Space3D/": "blue"
}

[gui]

fonts/dynamic_fonts/use_oversampling=false

