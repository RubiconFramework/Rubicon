; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Rubicon"
config/description="hi"
config/version="1"
config/tags=PackedStringArray("2d", "fnf", "framework", "rhythm")
run/main_scene="res://source/menus/title/Title.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Rubicon/Engine"
config/features=PackedStringArray("4.2", "C#", "Mobile")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://assets/misc/fnf.png"

[audio]

buses/default_bus_layout="res://assets/themes/default_bus_layout.tres"

[autoload]

Main="*res://source/Main.cs"
Conductor="*res://source/backend/autoload/Conductor.cs"
RubiconSettings="*res://source/backend/autoload/RubiconSettings.cs"
DebugInfo="*res://source/backend/autoload/DebugInfo.tscn"
LoadingHandler="*res://source/backend/autoload/LoadingHandler.cs"
ChartHandler="*res://source/backend/autoload/ChartHandler.cs"
AudioManager="*res://source/backend/autoload/AudioManager.tscn"

[debug]

file_logging/enable_file_logging=true

[display]

window/size/viewport_width=1366
window/size/viewport_height=768
window/subwindows/embed_subwindows=false
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_height"
window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="Rubicon"

[editor]

naming/node_name_num_separator=1
naming/default_signal_callback_name="On{signal_name}"
naming/default_signal_callback_to_self_name="On{signal_name}"
naming/scene_name_casing=1
export/convert_text_resources_to_binary=false

[editor_plugins]

enabled=PackedStringArray("res://addons/flashimport/plugin.cfg")

[file_customization]

folder_colors={
"res://export/": "gray"
}

