config_version=5

[application]

config/name="Rubicon"
config/version="1"
run/main_scene="res://TestScene.tscn"
config/use_custom_user_dir=true
config/features=PackedStringArray("4.3", "C#", "Forward Plus")
run/max_fps=165
run/delta_smoothing=false
config/icon="res://Assets/UI/Misc/fnf.png"

[autoload]

SettingsStorage="*res://Source/Rubicon.Core/Autoload/SettingsManager.cs"
DebugInfo="*res://Source/Rubicon.Core/Autoload/DebugInfo.tscn"
Conductor="*res://Source/Rubicon.Core/Autoload/ConductorSingleton.cs"
RubiconEngine="*res://Source/Rubicon.Core/Autoload/RubiconEngineSingleton.cs"

[display]

